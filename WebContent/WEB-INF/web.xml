	<servlet-mapping>
		<servlet-name>StudentSentMessages</servlet-name>
		<url-pattern>/student/sentmessages</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StudentNewMessage</servlet-name>
		<url-pattern>/student/newmessage</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LecturerReceivedMessages</servlet-name>
		<url-pattern>/lecturer/receivedmessages</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LecturerSentMessages</servlet-name>
		<url-pattern>/lecturer/sentmessages</url-pattern>
