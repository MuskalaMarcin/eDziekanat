	<servlet>
		<servlet-name>StudentTranscript</servlet-name>
		<jsp-file>/student/transcript.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminIndex</servlet-name>
		<url-pattern>/admin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StudentIndex</servlet-name>
		<url-pattern>/student</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LecturerIndex</servlet-name>
		<url-pattern>/lecturer</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginForm</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>403Error</servlet-name>
		<url-pattern>/403</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>404Error</servlet-name>
		<url-pattern>/404</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StudentApplications</servlet-name>
		<url-pattern>/student/applications</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StudentWaitingApplications</servlet-name>
		<url-pattern>/student/waiting_applications</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StudentNewApplication</servlet-name>
		<url-pattern>/student/newapplication</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
