		<url-pattern>/student/transcript</url-pattern>
	</servlet-mapping>
	<security-constraint>
		<display-name>HomeSecurityConstraint</display-name>
		<web-resource-collection>
			<web-resource-name>HomePage</web-resource-name>
			<url-pattern>/home</url-pattern>
			<url-pattern>/sendmessage</url-pattern>
			<url-pattern>/messages</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>*</role-name>
		</auth-constraint>
		<user-data-constraint>
			<transport-guarantee>NONE</transport-guarantee>
		</user-data-constraint>
	</security-constraint>
	<security-constraint>
		<display-name>AdminSecurityConstraint</display-name>
		<web-resource-collection>
			<web-resource-name>AdminsPages</web-resource-name>
			<url-pattern>/admin/*</url-pattern>
			<url-pattern>/admin/newmessage/*</url-pattern>
			<url-pattern>/administrator/sentmessages/*</url-pattern>
			<url-pattern>/administrator/receivedmessages/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>admin</role-name>
		</auth-constraint>
		<user-data-constraint>
			<transport-guarantee>NONE</transport-guarantee>
		</user-data-constraint>
	</security-constraint>
	<security-constraint>
		<display-name>StudentSecurityConstraint</display-name>
		<web-resource-collection>
			<web-resource-name>StudentsPages</web-resource-name>
			<url-pattern>/student/*</url-pattern>
			<url-pattern>/studenttimetable/*</url-pattern>
			<url-pattern>/studenttranscript/*</url-pattern>
			<url-pattern>/studentsubjects/*</url-pattern>
			<url-pattern>/studentscholarships/*</url-pattern>
			<url-pattern>/studentpayments/*</url-pattern>
			<url-pattern>/studentwaitingpayments/*</url-pattern>
			<url-pattern>/studentapplications/*</url-pattern>
			<url-pattern>/studentnewapplication/*</url-pattern>
			<url-pattern>/studentgetlecturers/*</url-pattern>
			<url-pattern>/studentlecturers/*</url-pattern>
			<url-pattern>/student/newmessage/*</url-pattern>
			<url-pattern>/student/sentmessages/*</url-pattern>
			<url-pattern>/student/receivedmessages/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>student</role-name>
		</auth-constraint>
		<user-data-constraint>
			<transport-guarantee>NONE</transport-guarantee>
		</user-data-constraint>
	</security-constraint>
	<security-constraint>
		<display-name>LecturerSecurityConstraint</display-name>
		<web-resource-collection>
			<web-resource-name>LecturersPages</web-resource-name>
			<url-pattern>/lecturer/*</url-pattern>
			<url-pattern>/lecturer/newmessage/*</url-pattern>
			<url-pattern>/lecturer/sentmessages/*</url-pattern>
			<url-pattern>/lecturer/receivedmessages/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>lecturer</role-name>
		</auth-constraint>
		<user-data-constraint>
			<transport-guarantee>NONE</transport-guarantee>
		</user-data-constraint>
	</security-constraint>
	<login-config>
		<auth-method>FORM</auth-method>
		<form-login-config>
			<form-login-page>/login</form-login-page>
			<form-error-page>/login</form-error-page>
		</form-login-config>
	</login-config>
	<security-role>
		<role-name>admin</role-name>
	</security-role>
	<security-role>
		<role-name>student</role-name>
	</security-role>
	<security-role>
		<role-name>lecturer</role-name>
	</security-role>
	<error-page>
		<error-code>403</error-code>
		<location>/403</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/404</location>
	</error-page>
	<resource-ref>
		<res-ref-name>jdbc/edziekanat</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	<welcome-file-list>
		<welcome-file>home</welcome-file>
	</welcome-file-list>
</web-app>